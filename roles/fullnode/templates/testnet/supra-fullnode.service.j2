[Unit]
Description=Supra fullnode

[Service]
User={{ service_identifier }}
Group={{ service_identifier }}
EnvironmentFile={{ fullnode_env_dir }}/normal_env_var
WorkingDirectory={{ fullnode_config_dir }}
ExecStart={{ fullnode_bin_dir }}/{{ service_identifier }}
Restart=always
RestartSec=4
LimitNOFILE=65536
StandardError=inherit

[Install]
WantedBy=multi-user.target