server:
  http_listen_port: {{ promtail.ports.http_listen_port }}
  grpc_listen_port: 0

positions:
  filename: {{ promtail_home_dir }}/positions-supra.yml

clients:
  - url: https://loki.services.supra.com/loki/api/v1/push

scrape_configs:
  - job_name: smr
    static_configs:
      - targets:
        - localhost
        labels:
          job: {{ promtail.job_name }}
          __path__: {{ log_path }}